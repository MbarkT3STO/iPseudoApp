# Test file for the new Constant keyword feature
# This file demonstrates the usage of the new Constant keyword

# Traditional const declaration
const PI = 3.14159

# New Constant keyword declaration  
Constant e = 2.71828
Constant gravity = 9.81

# Test mixed case variations
constant maxValue = 100
CONSTANT minValue = 0

# Example usage in calculations
var radius = 5
var area = PI * radius * radius
var force = gravity * 10

print "PI =", PI
print "e =", e 
print "Gravity =", gravity
print "Max Value =", maxValue
print "Min Value =", minValue
print "Area =", area
print "Force =", force
