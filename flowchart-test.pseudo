# Test pseudocode for flowchart generation
# This demonstrates branching, loops, and nested structures

var number = 5
var result = 1

# Simple if-else statement
if number > 0 then
    print "Number is positive"
else
    print "Number is not positive"
endif

# For loop with break and continue
for i = 1 to 10
    if i == 5 then
        break
    endif
    
    if i % 2 == 0 then
        continue
    endif
    
    print "Odd number:", i
endfor

# While loop with nested if
var counter = 0
while counter < 5
    if counter == 3 then
        print "Counter reached 3"
    else
        print "Counter:", counter
    endif
    
    counter = counter + 1
endwhile

# Complex nested structure
var score = 85

if score >= 90 then
    print "Grade: A"
elseif score >= 80 then
    print "Grade: B"
    
    # Nested loop inside elseif
    for j = 1 to 3
        print "Good job!"
    endfor
elseif score >= 70 then
    print "Grade: C"
else
    print "Grade: F"
    
    # Nested if inside else
    if score < 50 then
        print "You need to study more"
    endif
endif

print "Program completed"
